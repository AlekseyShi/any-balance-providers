<?xml version="1.0" encoding="utf-8"?>
<provider>
	<api flags="no_browser" />
	<id version="1">ab-tracking-chameleon</id>
	<name>Трекер Хамелеон</name>
	<description html="true"><![CDATA[
	<p>Автомобильный трекер, автономный маяк и персональный трекер «Хамелеон».</p>
	<p>Информация берётся с сайта http://gde.mychameleon.ru/</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="watchName" name="Объект наблюдения" type="text"/>
		<counter id="loc" name="Местоположение" type="text"/>
		<counter id="voltage" name="Заряд батареи" type="number"/>
		<counter id="balance" name="Баланс" type="number"/>
		<counter id="time" name="Время опроса" type="number"/>
		<counter id="gps_strength" name="Cигнал GPS/Glonass" type="number"/>
		<counter id="gsm_strength" name="Cигнал GSM" type="number"/>
		<counter id="altitude" name="Высота" type="number"/>
		<counter id="temperature" name="Температура" type="number"/>
	</counters>
	<keywords>автомобильный трекер, автономный маяк, персональный трекер</keywords>
	<type>tracking</type>
	<country>ru</country>
</provider>