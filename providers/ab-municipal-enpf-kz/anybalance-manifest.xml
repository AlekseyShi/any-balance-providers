		<counter id="uid" name="ИИН"/>
		<counter id="full_name" name="ФИО" type="text"/>
		<counter id="birth_date" name="Дата рождения" type="time" format="dd/MM/yyyy"/>
	</counters>
	<keywords></keywords>
	<type></type>
	<country>ru</country>
